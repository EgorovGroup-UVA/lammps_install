variable F atom 2.38/((x-5)^2/0.5^2+1)/0.5-2.38/((x+5)^2/${dlt}^2+1)/${dlt}
variable F atom 2.38/((x-5)^2/0.5^2+1)/0.5-2.38/((x+5)^2/0.5^2+1)/${dlt}
variable F atom 2.38/((x-5)^2/0.5^2+1)/0.5-2.38/((x+5)^2/0.5^2+1)/0.5
